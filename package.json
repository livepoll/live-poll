{
  "name": "live-poll",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build",
    "build:de": "ng build --configuration=de",
    "build:fr": "ng build --configuration=fr",
    "build:prod": "ng build --prod",
    "build:prod:de": "ng build --prod --configuration=de",
    "build:prod:fr": "ng build --prod --configuration=fr",
    "build:ci": "npm run build:prod",
    "int:extract": "ng xi18n --output-path src/locale --format=xlf2",
    "start": "ng serve",
    "start:de": "ng serve --configuration=de --open -- --port=4201",
    "start:fr": "ng serve --configuration=fr --open -- --port=4202"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.2",
    "@angular/common": "~10.1.2",
    "@angular/compiler": "~10.1.2",
    "@angular/core": "~10.1.2",
    "@angular/forms": "~10.1.2",
    "@angular/localize": "^10.1.3",
    "@angular/platform-browser": "~10.1.2",
    "@angular/platform-browser-dynamic": "~10.1.2",
    "@angular/router": "~10.1.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular/cli": "~10.1.4",
    "@angular/compiler-cli": "~10.1.2",
    "@types/node": "^14.11.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
